<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.agostinho.sicredimobile">

    <application
        android:name=".util.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activites.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activites.NovaContaActivity" />
        <activity android:name=".activites.PrincipalActivity" />
        <activity android:name=".activites.SaldoActivity" />
        <activity android:name=".activites.ExtratoActivity" />
        <activity android:name=".activites.DepositoActivity" />
        <activity android:name=".activites.SaqueActivity" />
        <activity android:name=".activites.TransferenciaActivity" />
        <activity android:name=".activites.EsqueceuSenhaActivity"></activity>
    </application>

</manifest>